{
  "name": "TrickTrick/laravel-json-schema-validate-fork",
  "description": "json schema for request and response",
  "keywords": ["laravel", "json schema", "api"],
  "license": "MIT",
  "authors": [
    {
      "name": "Alexey",
      "email": "alexeymarkov.x7@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "justinrainbow/json-schema": "^5.2.8",
    "illuminate/support": "^7.0",
    "illuminate/http": "^7.0",
    "illuminate/routing": "^7.0"
  },
  "require-dev": {
    "laravel/framework": "^7.0",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "Kojirock5260\\JsonSchemaValidate\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kojirock5260\\JsonSchemaValidate\\JsonSchemaServiceProvider"
      ]
    }
  },
  "prefer-stable": true
}
